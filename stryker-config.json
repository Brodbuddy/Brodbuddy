{
  "stryker-config": {
    "ignore-methods": [
      "Log*", 
      "Console.Write*",
      "*Exception.ctor"
    ],
    "mutate": [
      "/**/*.cs",
      "!/**/*.g.cs",
      "!/**/*.generated.cs",
      "!/**/PgDbContext.cs",
      "!/**/tests/**/*",
      "!/**/Extensions.cs",
      "!/**/AppOptions.cs",
      "!/**/FluentEmailSender.cs",
      "!/**/MessageWrapper.cs",
      "!/**/RedisSocketKeys.cs",
      "!/**/Controllers/**/*",
      "!/**/MessageHandlers/**/*",
      "!/**/EventHandlers/**/*",
      "!/**/*Exception.cs",
      "!/**/SwaggerExtensions.cs"
    ],
    "reporters": ["html", "progress"],
    "coverage-analysis": "perTest"
  }
}